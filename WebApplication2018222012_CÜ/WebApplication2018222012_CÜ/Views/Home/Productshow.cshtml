@model WebApplication2018222012_CÜ.Models.Product

<!DOCTYPE html>
<html lang="en-US">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <title>Medigo Blue, Project Image</title>
    <meta name="keywords" content="">
    <meta name="description" content="">
    <meta name="author" content="templatemo">
    <!--
    Medigo Template
    http://www.templatemo.com/preview/templatemo_460_medigo
    -->
    <!-- Google Fonts -->
    <link href="http://fonts.googleapis.com/css?family=PT+Serif:400,700,400italic,700itali" rel="stylesheet">
    <link href="http://fonts.googleapis.com/css?family=Raleway:400,900,800,700,500,200,100,600" rel="stylesheet">

    <!-- Stylesheets -->
    <link rel="stylesheet" href="~/blue/bootstrap/bootstrap.css">
    <link rel="stylesheet" href="~/blue/css/misc.css">
    <link rel="stylesheet" href="~/blue/css/blue-scheme.css">

    <!-- JavaScripts -->
    <script src="~/blue/js/jquery-1.10.2.min.js"></script>
    <script src="~/blue/js/jquery-migrate-1.2.1.min.js"></script>

    <link rel="shortcut icon" href="~/blue/images/favicon.ico" type="image/x-icon" />

</head>
<body>


    <div class="first-widget parallax" id="portfolioId">
        <div class="parallax-overlay">
            <div class="container pageTitle">
                <div class="row">
                    <div class="col-md-6 col-sm-6">
                        <h2 class="page-title">@Model.Productname</h2>
                    </div> <!-- /.col-md-6 -->
                    <div class="col-md-6 col-sm-6 text-right">
                        <span class="page-location">Home / @Model.Productname</span>
                    </div> <!-- /.col-md-6 -->
                </div> <!-- /.row -->
            </div> <!-- /.container -->
        </div> <!-- /.parallax-overlay -->
    </div> <!-- /.pageTitle -->

    <div class="container">
        <div class="row project-single">
            <div class="col-md-8">
                <div class="project-img">
                    <img src="@Model.ProductImg" alt=@Model.Id>
                </div> <!-- /.project-img -->
            </div> <!-- /.col-md-8 -->
            <div class="col-md-4">
                <div class="project-info">
                    <h3 class="project-title">@Model.Productname</h3>
                    <p class="description">@Model.Description</p>
                    <ul class="meta-post">

                        <li><span>Skills:</span>@Model.ProductCat</li>
                    </ul>
                </div> <!-- /.project-info -->
            </div> <!-- /.col-md-4 -->
        </div> <!-- /.row -->
    </div> <!-- /.container -->
    <!-- Scripts -->
    <script src="~/blue/js/min/plugins.min.js"></script>
    <script src="~/blue/js/min/medigo-custom.min.js"></script>

</body>
</html>
